<ul class="toolbar">
<% if user_signed_in? -%>
  <li>Logged in as <%= current_user.email %></li>
  <li><%= link_to "Log out", destroy_user_session_path %></li>
<% else -%>
  <li><%= link_to "Log in", new_user_session_path %></li>
  <li><%= link_to "Sign up", new_user_registration_path %></li>
<% end -%>
</ul>
